ip: 192.168.10.11
memory: 2048
cpus: 1
name: Ticketr
provider: virtualbox
authorize: ~/.ssh/id_rsa.pub
keys:
  - ~/.ssh/id_rsa
folders:
  - map:
    to: /home/vagrant/code
    type: nfs
sites:
  - map: ticketr.test
    to: /home/vagrant/code/public
databases:
  - ticketr
features:
  - mariadb: true
